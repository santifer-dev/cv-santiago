{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' https://vercel.live; style-src 'self' 'unsafe-inline' https://vercel.live; font-src 'self' https://vercel.live; img-src 'self' data: https:; connect-src 'self' https://vercel.live https://*.pusher.com wss://*.pusher.com; frame-src https://vercel.live; frame-ancestors 'none'; form-action 'self'; base-uri 'self'; object-src 'none'"
        },
        {
          "key": "X-Open-To",
          "value": "interesting-problems"
        },
        {
          "key": "X-Built-With",
          "value": "Claude Code"
        },
        {
          "key": "X-Evals",
          "value": "39"
        },
        {
          "key": "X-Resume",
          "value": "/llms.txt"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/en", "destination": "/en/index.html" },
    { "source": "/n8n-para-pms", "destination": "/n8n-para-pms/index.html" },
    { "source": "/n8n-for-pms", "destination": "/n8n-for-pms/index.html" },
    { "source": "/agente-ia-jacobo", "destination": "/agente-ia-jacobo/index.html" },
    { "source": "/ai-agent-jacobo", "destination": "/ai-agent-jacobo/index.html" },
    { "source": "/business-os-para-airtable", "destination": "/business-os-para-airtable/index.html" },
    { "source": "/business-os-for-airtable", "destination": "/business-os-for-airtable/index.html" },
    { "source": "/seo-programatico", "destination": "/seo-programatico/index.html" },
    { "source": "/programmatic-seo", "destination": "/programmatic-seo/index.html" }
  ],
  "crons": [
    {
      "path": "/api/cron/evaluate",
      "schedule": "0 8 * * *"
    }
  ]
}
